{
  "name": "prototype",
  "label": "PROTOTYPE",
  "version": "0.1.0",
  "author": "nsgroup",

  "assets": {
    "css": ["./css/prototype.css"] e 
  },

  "wrapper": "<div class='container'><div class='wrapper'><div id='detail' class='clearfix'><article class='article doc-section'></article></div></div></div>",

"imageRatios": {
  "16:9": {
    "label": "16:9",
    "ratio": "16x9"
  },
  "1:1": {
    "label": "1:1",
    "ratio": "1x1"
  }
},  
  
  
  "componentProperties": {
    "well-large": {
      "label": "Large",
      "type": "option",
      "value": "well-lg"
    },
	"paragraph-cols": {
      "label": "Columns",
      "type": "select",
      "options": [
      {
          "caption": "1 Column"
      },	  
	  {
		"caption":"2 Columns",
		"value": "ns_two-cols"
	  }	  
	  ]
    },
	"quote-size": {
      "label": "Small",
      "type": "option",
      "value": "ns_half"
    },
	"separator-size": {
      "label": "Big",
      "type": "option",
      "value": "ns_big"
    },	
	"dropcap": {
      "label": "Dropcap",
      "type": "option",
      "value": "ns_dropcap"
    },		
	"image-align": {
      "label": "Align",
      "type": "select",
      "options": [
      {
        "caption": "Left"
      },	  
	  {
		"caption":"Right",
		"value": "ns_right"
	  },
	  	  {
		"caption":"Middle",
		"value": "ns_middle"
	  },
	  {
		"caption":"Full",
		"value": "ns_full"
	  }	  
	  ]
    },
	"info-align": {
      "label": "Align",
      "type": "select",
      "options": [
      {
        "caption": "Left"
      },	  
	  {
		"caption":"Right",
		"value": "ns_right"
	  }  
	  ]
    },	
    "panel-styles": {
      "label": "Panel Styles",
      "type": "select",
      "options": [
        {
          "caption": "Default"
        },
        {
          "caption": "Primary",
          "value": "panel-primary"
        },
        {
          "caption": "Success",
          "value": "panel-success"
        },
        {
          "caption": "Info",
          "value": "panel-info"
        },
        {
          "caption": "Warning",
          "value": "panel-warning"
        },
        {
          "caption": "Danger",
          "value": "panel-danger"
        }
      ]
    }
  },
  "groups": [
    {
      "label": "Headers",
      "components": ["hero", "h1", "h2", "h3"]
    }, 
	{
      "label": "Text",
      "components": ["p", "author", "lead", "quote"]
    }, 
	{
      "label": "Images",
      "components": ["image", "6images"]
    },
	{
      "label": "Gallery",
      "components": ["gallery", "image-slide"]
    },	
	{
      "label": "Embeds",
      "components": ["media"]
    }, 
	{
      "label": "Lists",
      "components": ["list-group", "list-group-item", "list-group-box-item"]
    }, 
	{
      "label": "Boxes",
      "components": ["separator", "panel", "well", "info"]
    }, 
	{
      "label": "Grid",
      "components": ["main-and-sidebar"]
    }, 
	{
      "label": "Teasers",
      "components": ["teaser"]
    }
  ],

  "defaultComponents": {
    "paragraph": "p",
    "image": "image"
  },
  
  "imageRatios": {
    "16:9": {
      "label": "16:9",
      "ratio": "16x9"
    },
    "1:1": {
      "label": "1:1",
      "ratio": "1x1"
    },
    "4:3": {
      "label": "4:3",
      "ratio": "4x3"
    },
    "3:4": {
      "label": "3:4",
      "ratio": "3x4"
    }
  },

  "defaultContent": [
    { "component": "hero" },
    { "component": "lead" },
	{ "component": "p" },
	{ "component": "p" },
	{ "component": "quote" },
	{ "component": "p" },
	{ "component": "separator" },
	{ "component": "p" },
	{ "component": "info" },
	{ "component": "p" },
	{ "component": "p" },
	{ "component": "p" },	
	{ "component": "image" },	
	{ "component": "p" },
	{ "component": "p" },	
	{ "component": "p" },
	{ "component": "separator" }	
  ],

  "prefilledComponents": {},

  "metadata": [
    {
      "identifier": "title",
      "type": "text",
      "matches": ["hero.title", "h1.title", "h2.title"]
    },
    {
      "identifier": "title",
      "type": "text",
      "matches": ["header.title"]
    }
  ]
}
